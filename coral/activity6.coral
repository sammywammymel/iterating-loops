integer currC
float equivalentF

// Loop variable initialization
currC = -10

// Loop expression
while currC <= 40
   equivalentF = (currC * 9.0 / 5.0) + 32.0
   
   Put currC to output
   Put " C it i" to output
   Put equivalentF to output
   Put " F\n" to output
   
   // Loop variable update
   currC = currC + 5